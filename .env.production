# Configurações de produção
VITE_NODE_ENV=production

# API URL - ajustar conforme necessário
VITE_API_URL=https://sua-api.herokuapp.com/api

# Configurações de build
VITE_BUILD_ANALYZE=false
VITE_BUILD_SOURCEMAP=false

# Configurações de cache
VITE_CACHE_CONTROL=public, max-age=31536000, immutable

# Configurações de compressão
VITE_COMPRESSION=true
VITE_GZIP=true
VITE_BROTLI=true

# Configurações de otimização
VITE_MINIFY=true
VITE_TREE_SHAKING=true
VITE_CODE_SPLITTING=true

# Configurações de monitoramento (opcional)
# VITE_SENTRY_DSN=your-sentry-dsn
# VITE_ANALYTICS_ID=your-analytics-id